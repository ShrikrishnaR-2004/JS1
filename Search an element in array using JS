<html>
<body>
<script>
let array=[];
let tofound=0;
let size;
let found=0;
size=Number(prompt("Enter the size of array:"));
for(let i=0;i<size;i++)
{
array[i]=Number(prompt("Enter the element of the array:"));
}

tofound=Number(prompt("Enter the element to be found in the array:"));


for(let i=0;i<size;i++)
{
if (tofound==array[i])
found=1;

}

if (found==1)
{
console.log("The element has been found");
}
else
{
console.log("The element has not been found");
}

</script>
</body>
</html>
